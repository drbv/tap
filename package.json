        "express": "~4.18.1",
        "express-pouchdb": "^4.2.0",
        "i": "^0.3.7",
        "leveldown": "^6.1.1",
        "morgan": "~1.10.0",
        "papaparse": "^5.3.2",
        "rxdb": "^13.0.3",
        "rxjs": "^7.5.6",
        "ts-node": "^10.9.1"
    },
    "devDependencies": {
        "@types/config": "3.3.0",
        "@types/express": "^4.17.14",
        "@types/leveldown": "^4.0.3",
        "@types/node": "^18.7.18",
        "@types/papaparse": "^5.3.5",
        "@types/pouchdb-adapter-leveldb": "^6.1.3",
        "@types/uuid": "^8.3.4",
        "concurrently": "^7.4.0",
        "nodemon": "^2.0.20",
