{
    "name": "tap-server",
    "version": "0.0.0",
    "private": true,
    "main": "dist/index.js",
    "scripts": {
        "server": "ts-node tap-server/src/server.ts",
        "client": "npm start --prefix tap-ui",
        "dev": "NODE_ENV=development concurrently \"npm run server\" \"npm run client\"",
        "build": "tsc --project ./",
        "clean": "rm *rxdb* _replicator _users pouch__all_dbs__"
    },
    "dependencies": {
        "@babel/polyfill": "^7.12.1",
        "axios": "^0.25.0",
        "config": "^3.3.6",
        "cookie-parser": "~1.4.5",
        "debug": "~4.3.2",
        "express": "~4.17.1",
        "express-pouchdb": "^4.2.0",
        "morgan": "~1.10.0",
        "papaparse": "^5.3.1",
        "pouchdb-adapter-node-websql": "^7.0.0",
        "rxdb": "^10.0.3",
        "rxjs": "^7.3.0",
        "ts-node": "^10.2.1",
        "websql": "^2.0.3"
    },
    "devDependencies": {
        "@types/config": "0.0.39",
        "@types/express": "^4.17.13",
        "@types/node": "^14.14.32",
        "@types/papaparse": "^5.2.6",
        "@types/pouchdb-adapter-node-websql": "^6.1.3",
        "concurrently": "^6.2.1",
        "nodemon": "^2.0.12",
        "tslint": "^6.1.3",
        "typescript": "^4.2.3"
    }
}
