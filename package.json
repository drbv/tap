{
    "name": "tap-server",
    "version": "0.0.0",
    "private": true,
    "main": "dist/index.js",
    "scripts": {
        "server": "ts-node tap-server/src/server.ts",
        "client": "npm start --prefix tap-ui-ts",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "build": "tsc --project ./",
        "clean": "rm ./data/* pouch__all_dbs__"
    },
    "dependencies": {
        "axios": "^1.2.3",
        "config": "^3.3.9",
        "cookie-parser": "~1.4.6",
        "debug": "~4.3.4",
        "express": "~4.18.2",
        "express-pouchdb": "^4.2.0",
        "leveldown": "^6.1.1",
        "morgan": "~1.10.0",
        "node-gyp-build": "^4.6.0",
        "papaparse": "^5.3.2",
        "pouchdb-adapter-idb": "^8.0.0",
        "pouchdb-adapter-memory": "^8.0.0",
        "rxdb": "^13.17.1",
        "rxjs": "^7.8.0",
        "ts-node": "^10.9.1"
    },
    "devDependencies": {
        "@types/config": "3.3.0",
        "@types/express": "^4.17.15",
        "@types/leveldown": "^4.0.3",
        "@types/node": "^18.11.18",
        "@types/papaparse": "^5.3.7",
        "@types/pouchdb-adapter-leveldb": "^6.1.3",
        "@types/uuid": "^9.0.0",
        "concurrently": "^7.6.0",
        "ea-state-machine": "^2.4.0",
        "nodemon": "^2.0.20",
        "pouchdb-adapter-leveldb": "^8.0.0",
        "prettier": "^2.8.3",
        "tslint": "^6.1.3",
        "typescript": "^4.9.4"
    }
}
